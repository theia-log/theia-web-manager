{
  "name": "theia-web-manager",
  "version": "0.1.0",
  "description": "",
  "main": "server/index.js",
  "private": true,
  "scripts": {
    "start": "run-p client:start server:start",
    "serve:prod": "node build/server",
    "serve": "npm run build && node build/server",
    "build": "npm install && run-s client:build server:build && mkdir build/logs",
    "server:start": "nodemon -w server --exec \"ENV=development babel-node server\"",
    "server:build": "babel server -s -D -d build/server",
    "client:start": "react-scripts start",
    "client:build": "react-scripts build",
    "client:test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "core-decorators": "^0.20.0",
    "cors": "^2.8.4",
    "decorator-wrapper": "^1.0.0",
    "enum": "^2.5.0",
    "express": "^4.16.3",
    "express-ws": "^3.0.0",
    "history": "^4.7.2",
    "md5": "^2.2.1",
    "query-string": "^6.1.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "react-toastify": "^4.0.1",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.80.0",
    "shelljs": "^0.8.2",
    "signals": "^1.0.0",
    "url-pattern": "^1.0.3",
    "uuid": "^3.2.1",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.10.0",
    "nodemon": "^1.17.4",
    "npm-run-all": "^4.1.3"
  }
}
